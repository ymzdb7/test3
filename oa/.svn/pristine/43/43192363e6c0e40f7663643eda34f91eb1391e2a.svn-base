  <!--查询定时任务设置和记录-->
  <select id="selectTimedTask" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        SELECT
	ttm.*, ttr.TTR_ID,
	ttr.USER_ID,
	ttr.EXECUTION_TIME,
	ttr.RESULT
    FROM
	timed_task_management ttm
    INNER JOIN timed_task_record ttr ON ttm.TTM_ID = ttr.TTM_ID
    where 1=1
    <if test="ttmId !=null and ttmId!=0">
      AND  ttm.TTM_ID=#{ttmId}
    </if>
  </select>

  <!--查询定时任务设置-->
  <select id="selectTimeManager" resultMap="BaseResultMap">
          select *
          FROM (  SELECT
                   ttm.*, ttr.TTR_ID,
                   ttr.USER_ID,
                   ttr.EXECUTION_TIME,
                   ttr.RESULT
                  FROM
                      timed_task_record ttr,timed_task_management ttm
                  where   ttr.TTM_ID =ttm.TTM_ID
                  ORDER BY ttr.EXECUTION_TIME DESC) timeMang
          GROUP BY  timeMang.ttm_Id
  </select>

