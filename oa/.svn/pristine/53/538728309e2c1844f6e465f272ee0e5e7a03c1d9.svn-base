		</trim>
		)-->
	</select>
	<select id="detailedNews" resultMap="BaseResultMap" parameterType="Map" databaseId="oracle">
		SELECT
		n.NEWS_ID,
		n."SUBJECT",
		n.TYPE_ID,
		n.NEWS_TIME,
		n.CLICK_COUNT,
		n.PROVIDER,
		n. CONTENT,
		n.READERS,
		U .USER_NAME,
		n.PRIV_ID,
		n.USER_ID,
		n.TO_ID,
		n.PUBLISH,
		n.ATTACHMENT_ID,
		n.ATTACHMENT_NAME,
		n.TOP,
		n.TOP_DAYS,
		n.ANONYMITY_YN,
		U .AVATAR,
		U .SEX,
		n. SUMMARY,
		n.KEYWORD
		FROM
		 news n
		INNER JOIN 	"USER" U ON n.PROVIDER=u.USER_ID
		INNER JOIN  user_function uf ON uf.user_id=u.USER_ID
		WHERE
		n.NEWS_ID=#{newsId}
		and (
		<trim suffixOverrides="OR">
			<if test="userPriv != null and userPriv != ''">
				(n.PRIV_ID LIKE concat(concat('%,',#{userPriv}),',%')
				OR n.PRIV_ID LIKE concat(#{userPriv},',%')
				)OR
			</if>
			<if test="deptId != null and deptId != ''">
				(n.TO_ID LIKE concat(concat('%,',#{deptId}),',%')
				OR n.TO_ID LIKE concat(#{deptId},',%')
				)OR
			</if>
			<if test="userId != null and userId != ''">
				(n.USER_ID LIKE concat(concat('%,',#{userId}),',%')
				OR n.USER_ID LIKE concat(#{userId},',%')
				)OR
			</if>
			to_char(n.TO_ID) = 'ALL_DEPT'
			OR
			(uf.user_func_id_str LIKE  concat(concat('%,','105'),',%')
			OR uf.user_func_id_str LIKE concat('105',',%')
			)OR
		</trim>)


	</select>
