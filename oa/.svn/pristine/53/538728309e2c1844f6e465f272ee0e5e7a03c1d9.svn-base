	<select id="getNewCountByType" resultMap="BaseResultMap">
		SELECT
			n.NEWS_ID,n.`SUBJECT`,n.TYPE_ID,n.NEWS_TIME,n.CLICK_COUNT,n.PROVIDER,n.CONTENT,n.READERS,n.PRIV_ID,n.USER_ID,n.TO_ID,n.PUBLISH,n.ATTACHMENT_ID,n.ATTACHMENT_NAME,n.ANONYMITY_YN,n.TOP,s.CODE_NO,s.CODE_NAME
		FROM
			news as n
		LEFT JOIN
			(SELECT CODE_NO,CODE_NAME FROM sys_code WHERE sys_code.PARENT_NO="NEWS") AS s
		ON
			n.TYPE_ID = s.CODE_NO
		WHERE
		 	n.PUBLISH='1'
</select>
	<select id="getNewCountByType" resultMap="BaseResultMap" databaseId="oracle">
	SELECT
	n.NEWS_ID,
	n."SUBJECT",
  n.TYPE_ID,
	n.NEWS_TIME,
	n.CLICK_COUNT,
	n.PROVIDER,
	n."CONTENT",
	n.READERS,
	n.PRIV_ID,
	n.USER_ID,
	n.TO_ID,
	n.PUBLISH,
	n.ATTACHMENT_ID,
	n.ATTACHMENT_NAME,
	n.ANONYMITY_YN,
	n.TOP,
	s.CODE_NO,
	s.CODE_NAME
FROM
	news  n
LEFT JOIN (
	SELECT
		CODE_NO,
		CODE_NAME
	FROM
		sys_code
	WHERE
		sys_code.PARENT_NO = 'NEWS'
)  s ON n.TYPE_ID = s.CODE_NO
WHERE
	n.PUBLISH = '1'
</select>
