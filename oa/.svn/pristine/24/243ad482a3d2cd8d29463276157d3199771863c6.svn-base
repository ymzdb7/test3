/*开始*/
.startBegin{
    background: #50C625!important;
}

/*结束*/
.finish{
    background: #F4A8BD!important;
}


/*普通*/
.ordinary{
    background: #EEEEEE!important;
}


/*子流程节点*/
.cheil{
    background: #7D26CD;
}


/*外部节点*/
.external{
    background: #FFBC18;
}



/*未接收*/
.didNotReceive{
    background: #FFBC18!important;
}


/*办理中*/
.inProcess{
    background: #50C625!important;
}

/*挂其中*/
.hangUp{
    background: #7D26CD!important;
}


 /*办理完毕*/
.processed{
    background: #F4A8BD!important;
}


/*预设步骤*/
.theDefaultStep{
    background: #D7D7D7!important;
}

/*子流程*/
.cheold{
    background: #70A0DD!important;
}

