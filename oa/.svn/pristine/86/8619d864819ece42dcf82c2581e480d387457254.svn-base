   </select>

    <select id="getKgSignKeyList" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        SELECT
	ksk.*, GROUP_CONCAT(u.UID ORDER BY u.UID) USER_ID_STR,
	GROUP_CONCAT(u.USER_NAME ORDER BY u.UID) USER_NAME_STR
FROM
	kg_relation_keyuser krk
INNER JOIN kg_relation_keysign krs ON krs.KEY_ID = krk.KEY_ID
AND krs.SIGN_ID = #{signId}
INNER JOIN kg_signkey ksk ON ksk.SIGNKEY_ID = krk.KEY_ID
LEFT JOIN `user` u ON u.UID = krk.USER_ID
GROUP BY
	ksk.SIGNKEY_ID
    </select>
