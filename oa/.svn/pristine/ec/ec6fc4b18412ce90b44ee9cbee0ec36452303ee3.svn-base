@import url(m_reset.css);
html,
body,
body h1,h2,h3,div,span{
    font-family: '微软雅黑';
}
ul{
    list-style: none;
}
.outUl{
    margin-bottom: 1%;
}
.outLi{
    float: left;
    width: 31.3%;
    height:294px;
    border:#e0e0e0 1px solid;
    border-radius: 0px 0px 4px 4px;
    box-shadow: 0 2px 6px rgba(0,0,0,0.2);
    margin-left: 1.3%;
    margin-top: 1%;
    border-top: none;
}
.textLi{
    font-size: 11pt;
}
.titleLi img{
    margin: -5px 5px 0 10px;
    vertical-align: middle;
    /*margin-top: -3px;*/
}
.insideUl{
    float: right;
    margin-right: 10px;
}

.lineTop{
    display: block;
    width: 100%;
    height: 3px;
    background: #1c70d3;
}
.titleLi{
    height: 30px;
    line-height: 30px;
    width: 100%;
    background: #f0f7ff;
}
.insideUl li{
    float: left;
    height: 20px;
    padding: 0 5px;
    border:#3a93e6 1px solid;
    color: #3a93e6;
    background: #ffffff;
    line-height: 20px;
    margin-top: 4px;
    margin-left: 5px;
    border-radius: 4px;
    cursor: pointer;
}
.liActive{
    background: #3a93e6 !important;
    color: #ffffff !important;
    border: none !important;
    height:22px !important;
    line-height: 22px !important;
}


.insideDiv{
    width: 96%;
    margin: 20px auto;
    height: 220px;
}
.insideDiv ul{
    width: 100%;
    height:100%;
}
.insideDiv ul li{
    float: left;
    width: 31.3%;
    margin-left: 1.3%;
    height: 100%;
}
.userInfo{
    width: 100%;
    height: 50%;
    background: #dfedf8;
}
.userInfo2{
    width: 100%;
    height: 50%;
    background: #e7f5ff;
    box-sizing: border-box;
    padding-top: 10px;
}
.userImg{
    width: 40px;
    height: 40px;
    border-radius: 50%;
    border: #cccccc 1px solid;
    float: left;
    margin-top: 35px;
    margin-left: 3%;
}
.userImg img{
    width: 100%;
}
.userSpan{
    display: block;
    float: left;
    margin-top: 36px;
    /*margin-left: 10px;*/
    margin-left: 1%;
}
@media screen and (min-width: 1600px) {
    .userSpan {
        margin-left: 5%;
    }
}
.userSpan p:first-of-type{
    /*text-align: center;*/
    color: #6ba1df;
}
.userSpan p:last-of-type{
    /*text-align: center;*/
    color: #b3babd;
}
.userInfo2>span{
    display: block;
    margin-left: 5%;
    margin-top: 5px;
}

.title_s_can {
    position: relative;
    height: 230px;
}
.div_canvas {
    width: 50%;
    position: relative;
    height: 100%;
}
.div_canvas canvas {
    position: absolute;
    left: 23%;
    top: 10px;
    bottom: 0;
    margin: auto;
}
.div_prog {
    width: 50%;
    position: absolute;
    top: 0;
    right: 0;
    left: 50%;
}
.div_prog ul li {
    width: 80%;
    margin-top: 14px;
    margin-left: 15px;
    cursor: default;
}
.div_prog ul li span:first-of-type {
    display: inline-block;
    width: 56%;
}
.div_prog ul li span:nth-of-type(2) {
    display: inline-block;
    width: 44%;
}
.outer-therm {
    margin-top: 5px;
}
.therm {
    height: 30px;
    border-radius: 15px;
}
.moreData{
    float: right;
    height: 20px;
    padding: 0 5px;
    border: #3a93e6 1px solid;
    color: #3a93e6;
    background: #ffffff;
    line-height: 20px;
    margin-top: 4px;
    margin-right: 10px;
    border-radius: 4px;
    cursor: pointer;
}