.content{
	width: 100%;
	height: 100%;
	overflow: hidden;
	background-color: #fbfdfe;
}
.content .nav{
	width: 100%;
	height: 30px;
	background-color: #e8f5fd;
	border-bottom: #d7e0e8 1px solid;
	border-top: #d7e0e8 1px solid;
	overflow: hidden;
	
}
.content .nav li{
	font-size: 14px;
	float: left;
	width: 10%;
	text-align: center;
	height: 22px;
	line-height: 22px;
	margin-top: 3px;
	margin-right: 10px;
	cursor: pointer;
}
.content .nav li .spanTxt{
	width: 75%;
	display: block;
	text-align: center;
	line-height: 22px;
	height: 22px;
	color: #fff;
}
.content .nav li:first-of-type{
	margin-left: 3.4%;
	border: #7498b1 1px solid;
	background: url(../img/icon_up.png) no-repeat 90% center;
	background-color: #7ca9c9;
	border-radius: 3px;
	width: 60px;
}
.content .nav li:last-of-type{
	float: right;
	margin-top: 6px;
}
.noteMain{
	width: 100%;
	height: 1000px;
}
.noteMain .mainCont{
	width: 93.2%;
	/*height: 150px;*/
	margin: 20px auto;
	background-color: #fff;
	box-shadow: 3px 3px 10px #eee;
	border-radius: 5px;
	padding-bottom: 30px;
}
.noteMain .mainCont .title{
	width: 100%;
	height: 30px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.noteMain .mainCont .title span{
	display: inline-block;
	height: 100%;
	color: #fff;
	font-size: 14px;
	line-height: 30px;
	margin-left: 20px;
}
.noteMain .mainCont .mainContList p{
	margin-left: 20px;
	margin-top: 5px;
	font-size: 12px;	
}
.noteMain .mainCont .mainContList p:first-of-type{
	font-size: 14px;
	margin-top: 8px;
}
