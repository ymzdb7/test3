}

/*textarea自动高度*/
.autoTextarea {
	width: 400px;
	min-height: 120px;
	max-height: 300px;
	_height: 120px;
	padding: 3px;
	outline: 0;
	font-size: 16px;
	border: 1px solid #a0b3d6;
	word-wrap: break-word;
	overflow-x: hidden;
	overflow-y: auto;
	_overflow-y: visible;
}
/*toolbar的样式*/
.setHeight{
