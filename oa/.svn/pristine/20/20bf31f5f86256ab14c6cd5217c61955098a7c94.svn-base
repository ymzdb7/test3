/*BSwindow里的table样式 */
.search_btn{
	letter-spacing: 2px;
	padding:3px 6px;
	font-size: 12px;
	text-align: center;
	border:1px solid #0d93f6;
	outline:none;
	background-color: #fff;
	color:#000;
	outline: none;
	font-family: "Microsoft YaHei";
	cursor:pointer;
}
.TableBlock{
	border-collapse:collapse;
	background-color:#f4f4f4;
}
.TableBlock tr{
	border-bottom:1px solid #e5e5e5;
	line-height:35px;
}
.TableBlock tr>td>textarea{
