.btn-alert-gray{
	letter-spacing: 0;
	padding:0px 10px;
	font-size: 12px;
	line-height:24px;
	text-align:center;
	background-color: #d6d6d6;
	color:#000;
	outline: none;
	border:none;
	font-family: "Microsoft Yahei";
