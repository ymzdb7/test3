}
.TableBlock_page p{
	line-height: 25px;
}
.TableBlock_page tr{
	border-bottom: 1px solid #f2f2f2;
}
.TableBlock_page td{
	padding: 5px 0;
}
.TableBlock_page input[type=text]{
	min-height: 25px;
}
.TableBlock_page td input[type=text],.TableBlock_page td textarea {
    width: 35%;
}
