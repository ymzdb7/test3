    </select>
    <select id="queryByRunId" resultMap="FLowRunBase" databaseId="oracle">
        SELECT * FROM flow_run_prcs WHERE flow_run_prcs.RUN_ID=#{runId} and rownum >0 and  rownum &lt;= 1 ORDER BY flow_run_prcs.FLOW_PRCS DESC
    </select>



    <delete id="deleteByRunIds" parameterType="int">
        DELETE FROM flow_run_prcs  where RUN_ID in
        <foreach collection="list" index="index"  item="runIds" open="(" separator="," close=")">
            #{runIds}
        </foreach>
    </delete>
    
    
    <select id="checkGatherNode" parameterType="Map" resultType="Integer">
        SELECT DISTINCT 1 FROM flow_run_prcs WHERE RUN_ID = #{runId} AND FLOW_PRCS !=#{flowPrcs} AND OP_FLAG =1 AND FLOW_PRCS IN
        <foreach collection="listPrcs" index="index" item="otherFlowPrcs" open="(" separator="," close=")">
                          #{otherFlowPrcs}
        </foreach>
        AND PRCS_FLAG IN
        <foreach collection="listFlag" index="index" item="prcsFlags" open="(" separator="," close=")">
                     #{prcsFlags}
              </foreach>
    </select>
    
    
    <select id="checkMerging" parameterType="Map">
        SELECT * FROM flow_run_prcs WHERE PRCS_ID = #{prcsId} AND FLOW_PRCS = #{flowPrcs} AND PRCS_FLAG IN (1,2)
    </select>



    <select id="monitoring" resultMap="FLowRunBase" parameterType="Map">
        SELECT
        ft.FLOW_NAME,
        flowRun.RUN_NAME,flowRun.FLOW_ID,
        flowProcess.PRCS_NAME,
        u.USER_NAME,
        frp.*

        FROM
        flow_run flowRun,
        flow_run_prcs frp,
        flow_type ft,
        flow_priv fp,
        flow_process flowProcess,
        user u
        WHERE
        flowRun.RUN_ID = frp.RUN_ID
        AND flowRun.FLOW_ID = ft.FLOW_ID
        AND flowRun.FLOW_ID = fp.FLOW_ID
        AND ft.FLOW_ID = flowProcess.FLOW_ID
        AND flowProcess.PRCS_ID = frp.FLOW_PRCS
        AND frp.PRCS_FLAG =2
        AND frp.OP_FLAG = 1
        AND fp.PRIV_TYPE=2
        AND u.USER_ID = frp.USER_ID
        AND flowRun.END_TIME is NULL AND flowRun.DEL_FLAG = 0
        <if test="runId != null and runId != ''">  <!--流水号 -->
            and flowRun.RUN_ID=#{runId}
        </if>
        <if test="runName != null and runName != ''"> <!--工作名称/文号-->
            and flowRun.RUN_NAME like CONCAT('%',#{runName},'%')
        </if>
        <if test="flowName != null and flowName != ''"> <!--工作名称/文号-->
            and flowRun.RUN_NAME like CONCAT('%',#{flowName},'%')
        </if>
        <if test="workLevel != null and workLevel != ''"> <!--优先级-->
            and flow_run.work_level =#{workLevel}
        </if>
        <if test="userId != null and userId != ''">
            and frp.USER_ID=#{userId}
        </if>
        <if test="flowId != null and flowId != ''">
            and flowRun.FLOW_ID=#{flowId}
        </if>
        ORDER BY  flowRun.RUN_ID DESC
    </select>



