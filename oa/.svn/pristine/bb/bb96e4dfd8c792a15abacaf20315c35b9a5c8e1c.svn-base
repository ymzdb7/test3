    <select id="selectMyAll" resultMap="BaseResultMap" parameterType="Map">
        SELECT
        fr.RUN_ID,
        fr.WORK_LEVEL,
        fr.FLOW_ID,
        fr.RUN_NAME,
        fr.BEGIN_USER,
        fr.BEGIN_DEPT,
        frp.ID,
        frp.USER_ID,
        frp.PRCS_ID,
        frp.FLOW_PRCS,
        frp.COMMENT,
        frp.OP_FLAG,
        frp.TOP_FLAG,
        frp.PRCS_FLAG,
        frp.TIME_OUT_FLAG,
        frp.PRCS_TIME,
        frp.CREATE_TIME,
        frp.DELIVER_TIME,
        frp.TIME_OUT_TYPE,
        frp.TIME_OUT_ATTEND,
        frp.IS_REMIND,
        frp.TIME_OUT
        FROM
        FLOW_RUN_PRCS frp
        INNER JOIN FLOW_RUN  fr ON fr.RUN_ID = frp.RUN_ID
        where
        frp.CHILD_RUN = '0'
