  .Mod_ireve {
    height: 100px;
    position: relative;
  }

  .Mod_ireve .pull_left {
    display: flex;
    flex-direction: column;
    justify-content: center;
    height: 100%;
  }
  .Mod_ireve .sub{
    line-height: 1.0;
    color: #598FD6;
    font-size: 24px;
    margin-bottom: 10px;
  }

  .Mod_ireve .no_sub {
    line-height: 1.0;
    color: #FF6F6F;
    font-size: 24px;
    margin-bottom: 10px;
  }
  .Mod_ireve .pull_right {
    height: 100%;
    flex-direction: column;
    justify-content: center;
    display: flex;
  }

  .Mod_ireve::after{
    content: '';
    display: block;
    width: 1px;
    height: 40px;
    background-color: #eee;
    position: absolute;
    left: 50%;
    transform: translate(-50%, 0, 0);
    top: 35px;
  }
  .Mod_ireve .pull_left div:last-child,   .Mod_ireve .pull_right div:last-child {
    line-height: 1.0;
    color: #999;
  }
