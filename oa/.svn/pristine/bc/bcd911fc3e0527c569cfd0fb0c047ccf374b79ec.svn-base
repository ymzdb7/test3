@import url(m_reset.css);
html,
body,
.wrap{
	width:100%;
	height:100%;
	overflow: hidden;
}
.head{
	width:100%;
	height:45px;
	background:#fff;
	border-bottom:1px solid #000;
}
.head_left,.head_mid,.head_rig{
	float:left;
	height:100%;
}
.head_left{
	width: 24%;
}
.head_left h1{
	color: #2f80dd;
	font-size: 22px;
	height: 100%;
	line-height: 50px;
	text-align: center;
}
.inp{
	width: 200px;
	height: 27px;
	border-radius: 6px;
	margin-top: 10px;
	margin-left: 97px;
}
.search{
	/*width: 65px;*/
	/*height: 28px;*/
	/*background: #45c0cf;*/
	/*margin-top: 10px;*/
	/*margin-left: 30px;*/
	/*border-radius: 5px;*/
}
.search h1{
	text-align: center;
	color: #fff;
	font-size: 14px;
	line-height: 28px;
}
.head_mid{
	width: 60%;
}
.inp,.search{
	float:left;
}

.cont{
	width:100%;
	height:101%;
	overflow-y: scroll;
}

.cont_left{
	width:16%;
	height:100%;
	border-right:1px solid #000;
	overflow-y:scroll
}
.cont_rig{
	overflow-y:scroll;
	width:83%;
	height:100%;
}
.cont_left,.cont_rig{
	float:left;
}
