*{
	margin: 0;
	padding: 0;
}
body{
	font-family: "微软雅黑";
	position: relative;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
a{
	text-decoration: none;
	color: #2B7FE0;
}
ul{
	list-style: none;
}
.content{
	width: 100%;
	overflow: hidden;
}
.header{
	width: 600px;
	height: 150px;
	overflow: hidden;
	margin: 20px auto;
	background-color: #f7f7f7;
	border: #e9e9e9 1px solid;
}
.mainTitle{
	width: 100%;
	height: 40px;
	background-color: #2b7fe0;
}
.mainTitle>span{
	font-size: 16px;
	color: #ffffff;
	line-height: 40px;
	margin-left: 40px;
}
.header .imgHead{
	width: 120px;
	margin: 15px 0 15px 15px;
	float: left;
	margin-right: 4%;
}
.img{
	width: 120px;
	height: 120px;
}

.header .information{
	float: left;
}
.uName{
	margin-top: 20px;
	font-size: 16px;
	color: #333333;
	font-weight: bold;
	width: 80px;
	/*background: url("../../img/sys/icon_man.png") no-repeat;*/
}
/*.userImg{
	display: inline-block;
	height: 21px;
	width: 24px;
	background: url("../../img/sys/icon_man.png") no-repeat;
}*/
.header .information p{
	margin-top: 20px;
	font-size: 14px;
}

.footer{
	width: 600px;
	overflow: hidden;
	margin: 0 auto;
	border: #e9e9e9 1px solid;
	padding-bottom: 15px;
}
.title{
	width: 100%;
	height: 40px;
	line-height: 40px;
	margin-left: 15px;
	font-size: 16px;
	font-weight: bold;
}
.excelLeft{
	float: left;
	width: 273px;
	margin-left: 15px;
}
.excelRight{
	float: right;
	width: 273px;
	margin-right: 15px;
}
table{
	width: 100%;
	border-collapse:collapse;
	background-color: #fff;
	border: #e9e9e9 1px solid;
}
table tr td{
	border: none;
	font-size: 14px;
	padding: 8px;
}
table tr td:first-of-type{
	border-right: #e9e9e9 1px solid;
	background-color: #e8f4fc;
}
