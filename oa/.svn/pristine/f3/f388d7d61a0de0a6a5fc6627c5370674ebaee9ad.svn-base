
.over_f{
    border-left: 1px solid #919191;
}
.define_p{
    line-height: 48px;
    padding-left: 22px;
    background: #c7e1ff;
    font-size: 16px;
    font-weight: 900;
    color: #262f34;
    height: 48px;
}
.con_form{
    width: 70%;
    padding: 50px 0 0 20px;
}
.form_group{
    width: 100%;
    text-align: left;
}
.form_group label.flowName{
    font-size: 16px;
    font-weight: 900;
    vertical-align: 11px;
    margin-right: 2%;
    width: 22%;
    display: inline-block;
    text-align: right;
}
.form_group label span{
    margin-left: 4px;
    vertical-align: -3px;
    color: red;
}
.form_group input[type=text]{
    margin-bottom: 24px;
    font-size: 16px;
    width: 286px;
    height: 26px;
    padding: 0 3px;
    padding-left: 10px;
    font-size: 14px;
    border-radius: 4px;
    vertical-align: middle;
    border:1px solid #ddd;
}
.depName{
    width: 287px;
    height: 26px;
    padding: 0 3px;
    border-radius: 4px;
    margin-bottom: 24px;
    vertical-align: middle;
}
.form_group input[type='radio'] {
    text-align: right;
    padding-right: 50px;
    width: 16px;
    height: 16px;
    vertical-align: 6px;
    margin-bottom: 24px;
}
.form_group label{
    vertical-align: 8px;
}
.title3_label{
    font-size: 12px;
    margin-bottom: 10px;
}
.title_3{
    margin-bottom: 20px;
}
.form_group textarea{
    width: 218px;
    border-radius: 4px;
    resize: none;
    padding-left: 10px;
}
.select_put a{
    margin-left: 10px;
    cursor: pointer;
    color: #3994f7;
    font-size: 12px;
}
.clearfix:after{
    content:'';
    display: block;
    clear: both;
}
.form_group input[type=checkbox]{
    vertical-align: 8px;
}
.set_form{
    position: absolute;
    left: 20%;
    font-size: 14px;
    color: #db6b20;
}
.conForm_r{
    font-size: 12px;
    position: absolute;
    left: 660px;
    top:0;
}
.define_r {
    position: fixed;
    border-bottom:1px solid #919191;
    height: 48px;
    line-height: 48px;
    top:0;
    left:0;
    right:0;
    background: #fff;
    z-index:999999;
}
html,body{
    margin:0;
    padding:0;
}
.define_r span a.keepmsg{
    background: url(/img/btn_save_03.png) no-repeat;
    color: #fff;
    border-radius: 6px;
    position: absolute;
    right: 3px;
    cursor: pointer;
    height: 30px;
    line-height: 30px;
    font-size: 12pt;
    width: 100px;
    text-align: center;
    margin-top: 10px;
    text-decoration:none
}
