@CHARSET "UTF-8";

.clearfix:before, .clearfix:after {
  
    content: "";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1; /*IE/7/6*/
    
}

a {
   /*  color: #666;  */
    text-decoration: none;
}

a:hover {
    /* color: #C81623; */
}

h1, h2, h3, h4, h5, h6 {
    text-decoration: none;
    font-weight: normal;
    font-size: 100%;
}

s, i, em {
    
    font-style: normal; 
    text-decoration: none; 
}

.col-red {
    color: #C81623 !important; 
}


.w {
  
    width:100%;
    margin: 0 auto;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.al {
    text-align: left;
}

.ac {
    text-align: center;
}

.ar {
    text-align: right;
}

.hide {
    display: none;
}


