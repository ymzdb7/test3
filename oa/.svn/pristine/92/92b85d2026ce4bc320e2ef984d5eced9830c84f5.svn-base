.modal-ft{
    position: absolute;
    bottom: 0px;
    width: 100%;
    padding: 10px 0px;
    text-align: center;
    border-top: 1px solid #ddd;
    background: #eee;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}
.step3{
    position: static;
}

.modal-ft button{
    margin-right: 20px;
    padding: 6px 15px;
    cursor: pointer;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    line-height: 1.3;
    -webkit-transition: .3s;
    -moz-transition: .3s;
    -ms-transition: .3s;
    -o-transition: .3s;
    transition: .1s;
    overflow: hidden;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    font-weight: 500;
    outline: none;
    border: 0px;
    border: 1px solid #C9C9C9;
}
#qyfb label{width: 135px;}
.modal-ft button.ok{border-color:#25CAE8;color: #fff;background-color: #25CAE8;}
.modal-ft button.ok:hover{border-color:#69D4E7;color: #fff;background-color: #69D4E7;}
.modal-ft button.cancel{color: #292929;background-color: #f8f8f8;}
.modal-ft button.cancel:hover{color: #292929;background-color: #FFFFFF;}
.modal-bd input{
    background-color: #fff;
    border: 1px solid #ccc;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
    -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
    box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
    -webkit-transition: border linear .2s,box-shadow linear .2s;
    -moz-transition: border linear .2s,box-shadow linear .2s;
    -o-transition: border linear .2s,box-shadow linear .2s;
    transition: border linear .2s,box-shadow linear .2s;
    height: 20px;
    padding: 4px 6px;
    margin-bottom: 10px;
    font-size: 14px;
    line-height: 20px;
    color: #555;
    vertical-align: middle;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}
.modal-bd input:focus{
    border-color: rgba(82,168,236,0.8);
    outline: 0;
    outline: thin dotted \9;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6);
    -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6);
    box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6);
}
