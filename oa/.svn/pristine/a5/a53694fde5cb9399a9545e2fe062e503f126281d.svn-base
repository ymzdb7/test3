            <if test="name != null">
                MENU_NAME = #{name,jdbcType=VARCHAR},
            </if>
            <if test="name1 != null">
                MENU_NAME1 = #{name1,jdbcType=VARCHAR},
            </if>
            <if test="name2 != null">
                MENU_NAME2 = #{name2,jdbcType=VARCHAR},
            </if>
            <if test="name3 != null">
