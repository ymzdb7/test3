            </if>
            <if test="name5 != null">
                MENU_NAME5 = #{name5,jdbcType=VARCHAR},
            </if>
            <if test="ext != null">
                MENU_EXT = #{ext,jdbcType=VARCHAR},
            </if>
            <if test="img != null">
                IMAGE = #{img,jdbcType=VARCHAR},
            </if>
        </set>
        where MENU_ID = #{id,jdbcType=VARCHAR}
    </update>

