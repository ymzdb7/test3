  <select id="getSupDetail" resultMap="UserResultMap" parameterType="com.xoa.model.supervision.SupFeedBack">
    SELECT
	sb.content,
	sb.creater_id,
	sb.create_time,
	sb.sup_id,
	sb.sid,
	sb.level_,
	sb.title,
	u.USER_NAME
FROM
	sup_feed_back sb,
	USER u,
supervision s
