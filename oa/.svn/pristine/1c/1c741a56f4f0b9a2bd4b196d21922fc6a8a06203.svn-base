      <if test="userRange!=null and userRange!=''">
        OR USER_RANGE like CONCAT(CONCAT('%',#{userRange}),'%')
      </if>
      <if test="deptRange!=null and deptRange!='' and userRange!=null and userRange!='' ">
        OR (DEPT_RANGE like CONCAT(CONCAT('%',#{deptRange}),'%') and USER_RANGE like CONCAT(CONCAT('%',#{userRange}),'%')）
      </if>
    </where>
  </select>

</mapper>