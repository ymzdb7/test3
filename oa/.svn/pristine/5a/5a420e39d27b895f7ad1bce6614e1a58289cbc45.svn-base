
.clearfix:after {
  content: " ";
  display: block;
  clear: both;
  height: 0;
}
.clearfix {
  zoom: 1;
}
.mo_box {
  width: 94%;
  margin: 0 auto;
  height: 170px;
	box-sizing: border-box;
	color: #666;
	line-height: 1.0;
	position: relative;
	border-radius: 10px;
	margin-bottom: 14px;
	letter-spacing: 2px;
	overflow: hidden;
}

.Modular {
  margin-top: 0.22rem;
}
