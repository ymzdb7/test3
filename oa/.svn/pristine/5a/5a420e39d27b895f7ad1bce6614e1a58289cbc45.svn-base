.mo_box h4{
      font-size: 18px;
    	font-weight: normal;
    	display: inline-block;
	    color: #fff;
}

.mo_daily {
  background-color: #fff;
}

.mo_week {
  background-color: #fff;
}

.mo_month {
  background-color: #fff;
}
.box_count {
	  position: relative;
    font-size: 16px;
    width: 90%;
    margin: 20px auto 10px;
    height: 30px;
    line-height: 26px;
}
.box_count .Arrow {
	  position: absolute;
	  top: 0;
	  right: 0;
     border: 1px solid #6cc76f;
    border-radius: 4px;
    font-size: 14px;
    color: #6cc76f;
    font-weight: bold;
    letter-spacing: 2px;
    padding: 2px 8px;
}
.pull_left {
	float:left;
}
.pull_right {
	float:right;
}
.btn_list {
    border: 1px solid #fff;
    border-radius: 4px;
    font-size: 14px;
    color: #fff;
    font-weight: bold;
    padding: 6px;
    position: absolute;
    left: 16px;
    bottom: 10px;
}
/*我收到模块统计页*/
